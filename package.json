    "express": "^4.15.4",
    "fullcalendar": "^3.9.0",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "mongoose": "^5.0.17",
    "react-big-calendar": "^0.19.2",
    "react-bootstrap": "^0.32.1",
    "react-burger-menu": "^2.5.2",
    "react-custom-scrollbars": "^4.2.1",
